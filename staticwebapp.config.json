{
  "platform": {
    "apiRuntime": "node:18"
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=3600"
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*"]
  }
}